var x1; var x2; var x3; var x4; var x5; var x6;
maximize profit: 60*x1 + 40*x2 + 30*x3 + 30*x4 + 15*x5 + 60*x6;
subject to total: x1 + x2 + x3 + x4 + x5 + x6 <= 7;
subject to disk: x2 + x4 <= 3;
subject to 256k: 4*x1 + 2*x2 + 2*x3 + 2*x4 + x5 <= 8;
subject to alter: 2*x6 <= 4;
subject to g1: x1 + x6 <= 1.8;
subject to g3: x3 <= 0.3;
subject to g123: x1 + x2 + x3 + x6 <= 3.8;
subject to w12: x4 + x5 <= 3.2;
subject to g2: x2 >= 0.5;
subject to w1: x4 >= 0.5;
subject to w2: x5 >= 0.4;
subject to ge1: x1 >= 0;
subject to ge3: x3 >= 0;
subject to ge6: x6 >= 0;
option solver cplex;
option cplex_options 'sensitivity';
solve;
display profit;
display x1,x2,x3,x4,x5,x6;
display profit.up, profit.down;
display _varname, _var.rc, _var.down, _var.current, _var.up;
display _conname, _con.body, _con.slack, _con.dual, _con.down, _con.current, _con.up;